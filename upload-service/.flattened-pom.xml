  </parent>
  <groupId>com.handmade.tle</groupId>
  <artifactId>upload-service</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>upload-service</name>
  <licenses>
    <license>
      <name>MIT License</name>
      <url>http://www.opensource.org/licenses/mit-license.php</url>
    </license>
