      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>com.handmade.tle</groupId>
      <artifactId>storage-service-api</artifactId>
      <version>${revision}</version>
    </dependency>
    <dependency>
      <groupId>com.handmade.tle</groupId>
